        </rowConstraints>
         <children>
            <Button mnemonicParsing="false" onAction="#handlerBuilder" text="Button" GridPane.columnIndex="2" />
            <Label text="Label" />
            <TextField GridPane.columnIndex="1" />
            <Text fx:id="outputText" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
   </children>
